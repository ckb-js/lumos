{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Lumos",
  "out": "website/static/api",
  "includeVersion": true,
  "entryPoints": ["packages/*"],
  "entryPointStrategy": "packages",
  "exclude": [
    "packages/e2e-test",
    "packages/testkit",
    "packages/molecule",
    "packages/toolkit",
    "packages/crypto"
  ],
  "logLevel": "Verbose",
  "skipErrorChecking": true,
  "excludePrivate": true,
  "readme": "./assets/readme-typedoc.md"
}
